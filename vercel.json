{
    "rewrites": [
          { "source": "/api/competitors/:id/history", "destination": "/api/competitors/[id]/history" },
          { "source": "/api/competitors/:id", "destination": "/api/competitors/[id]" },
          { "source": "/api/offers/:id/checks", "destination": "/api/offers/[id]/checks" },
          { "source": "/api/offers/:id", "destination": "/api/offers/[id]" },
          { "source": "/api/checks/:id", "destination": "/api/checks/[id]" }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                { "key": "Access-Control-Allow-Origin", "value": "*" },
                { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,OPTIONS" },
                { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
            ]
        }
    ]
}
